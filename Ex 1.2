// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

int main() {
   char op;
    float num1 , num2;
    
    cout << "Enter operator : +,-,*,/:";
    cin >> op;
    
    cout << "Enter two operator : ";
    cin >> num1, num2;
    
    switch(op){
        case '+':
            cout << num1  << " + " << num2 << num1 + num2;
            break;
        case '-':
            cout << num1  << " - " << num2 << num1 - num2; 
            break;
        case '*':
            cout << num1  << " * " << num2 << num1 * num2; 
            break;
        case '/':
            cout << num1  << " / " << num2 << num1 / num2; 
            break;
        default: 
            cout << "Error! operator is  correct ";
            break;
    }
    return 0;
}
